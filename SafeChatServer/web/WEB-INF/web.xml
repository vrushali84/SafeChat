<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>userRegistration</servlet-name>
        <servlet-class>userRegistration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getDetails</servlet-name>
        <servlet-class>getDetails</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>onlineUsers</servlet-name>
        <servlet-class>onlineUsers</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>requestLogout</servlet-name>
        <servlet-class>requestLogout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>requestLogin</servlet-name>
        <servlet-class>requestLogin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>userRegistration</servlet-name>
        <url-pattern>/userRegistration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getDetails</servlet-name>
        <url-pattern>/getDetails</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>onlineUsers</servlet-name>
        <url-pattern>/onlineUsers</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>requestLogout</servlet-name>
        <url-pattern>/requestLogout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>requestLogin</servlet-name>
        <url-pattern>/requestLogin</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
